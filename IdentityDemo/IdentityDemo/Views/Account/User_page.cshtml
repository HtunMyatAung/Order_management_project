@{
}
@model IdentityDemo.Models.UserViewModel

@if (User.Identity.IsAuthenticated)
{
    <p>Welcome, @User.Identity.Name!</p>
}
else
{
    <div>
        <a href="~/Account/Login">Sign In</a>
        <span> or </span>
        <a href="~/Account/Register">Register</a>
    </div>
}
